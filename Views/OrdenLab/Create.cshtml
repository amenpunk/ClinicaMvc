@model Clinica.Models.OrdenLab

@{
    ViewData["Title"] = "Ordenes de Laboratorio";

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>OrdenLab</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NombreExamen" class="control-label"></label>
                <input asp-for="NombreExamen" class="form-control" />
                <span asp-validation-for="NombreExamen" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdConsulta" class="control-label"></label>
                <select asp-for="IdConsulta" class ="form-control" asp-items="ViewBag.IdConsulta"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
