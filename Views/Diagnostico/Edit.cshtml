@model Clinica.Models.Diagnostico

@{
    ViewData["Title"] = "Diagnostico";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>Diagnostico</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdDiagnostico" />
            <div class="form-group">
                <label asp-for="IdCie" class="control-label"></label>
                <input asp-for="IdCie" class="form-control" />
                <span asp-validation-for="IdCie" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdConsulta" class="control-label"></label>
                <select asp-for="IdConsulta" class="form-control" asp-items="ViewBag.IdConsulta"></select>
                <span asp-validation-for="IdConsulta" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

</body>
</html>
